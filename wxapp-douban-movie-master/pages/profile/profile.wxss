.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}

.userinfo {
  display: flex;
  flex-direction: column;
}

.userinfo-avatar {
  width: 300rpx;
  height: 300rpx;
  margin: 20rpx;
  border-radius: 260px;
}

.userinfo-nickname {
  color: #aaa;
}